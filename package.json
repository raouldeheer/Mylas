{
  "name": "@raouldeheer/mylas",
  "version": "1.1.1",
  "description": "Mylas is a npm package to make the loading and storing of data from fs easy and reliable.",
  "main": "build/index.js",
  "typings": "build/index",
  "files": [
    "build/"
  ],
  "scripts": {
    "prepack": "npm run build",
    "test": "jest --config='./tests/jest.config.js'",
    "build": "tsc --build ./config/tsconfig.json",
    "lint": "eslint -c ./config/.eslintrc --ext .ts ./ts",
    "dev": "npm run build && node ./build/",
    "start": "node ./build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raouldeheer/Mylas.git"
  },
  "keywords": [
    "fs",
    "data",
    "json",
    "loader"
  ],
  "author": "Raoul de Heer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raouldeheer/Mylas/issues"
  },
  "homepage": "https://github.com/raouldeheer/Mylas#readme",
  "devDependencies": {
    "@raouldeheer/tstypes": "^1.0.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "comlink": "^4.3.0"
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
