{
  "name": "mylas",
  "version": "1.2.1",
  "description": "Mylas is a npm package to make the loading and storing of data from fs easy and reliable.",
  "main": "build/index.js",
  "typings": "build/index",
  "files": [
    "build/"
  ],
  "scripts": {
    "prepack": "npm run build",
    "test": "jest --config='./tests/jest.config.js'",
    "build": "tsc --build ./config/tsconfig.json & tsc --build ./config/tsconfig2.json",
    "lint": "eslint -c ./config/.eslintrc --ext .ts ./ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raouldeheer/Mylas.git"
  },
  "keywords": [
    "fs",
    "data",
    "json",
    "loader",
    "multithreaded"
  ],
  "author": "Raoul de Heer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raouldeheer/Mylas/issues"
  },
  "homepage": "https://github.com/raouldeheer/Mylas#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
