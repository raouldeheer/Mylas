{
  "name": "mylas",
  "version": "2.1.4",
  "description": "Mylas is a npm package to make the loading and storing of data from fs easy and reliable.",
  "main": "build/index.js",
  "typings": "build/index",
  "files": [
    "build/"
  ],
  "sideEffects": false,
  "scripts": {
    "prepack": "npm run build",
    "test": "jest --config='./tests/jest.config.js'",
    "build": "bash ./config/build.sh",
    "lint": "eslint -c ./config/.eslintrc --ext .ts ./ts",
    "docs": "typedoc --excludeInternal --tsconfig config/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raouldeheer/Mylas.git"
  },
  "keywords": [
    "fs",
    "data",
    "json",
    "loader",
    "multithreaded"
  ],
  "author": "Raoul de Heer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raouldeheer/Mylas/issues"
  },
  "homepage": "https://mylas.js.org/",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^27.2.5",
    "ts-jest": "^27.0.5",
    "tsup": "^5.4.0",
    "typedoc": "^0.22.5",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/raouldeheer"
  }
}
